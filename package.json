{
  "name": "@fabdeh/eslint-config",
  "type": "module",
  "version": "0.4.0",
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "description": "My personal eslint config preset",
  "author": {
    "name": "Fabien DehoprÃ©",
    "email": "fabien@dehopre.dev",
    "url": "https://github.com/FabienDehopre"
  },
  "license": "MIT",
  "homepage": "https://eslint-config.dehopre.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FabienDehopre/eslint-config.git"
  },
  "bugs": "https://github.com/FabienDehopre/eslint-config/issues",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "lint",
    "formatting"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^20.11.1 || ^22.11.0"
  },
  "scripts": {
    "build": "unbuild",
    "build:inspector": "pnpm build && pnpx @eslint/config-inspector build",
    "dev": "pnpx @eslint/config-inspector --config eslint.config.ts",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || simple-git-hooks",
    "commit": "git-cz",
    "lint": "eslint . --fix",
    "lint:check": "eslint . --quiet",
    "prepack": "pnpm build",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@prettier/plugin-xml": "^3.4.1",
    "eslint": "^9.21.0",
    "eslint-plugin-erasable-syntax-only": "^0.3.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "prettier-plugin-slidev": "^1.0.5"
  },
  "peerDependenciesMeta": {
    "@prettier/plugin-xml": {
      "optional": true
    },
    "eslint-plugin-erasable-syntax-only": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "prettier-plugin-slidev": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@clack/prompts": "^0.10.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.26.0",
    "@eslint/markdown": "^6.4.0",
    "@ngrx/eslint-plugin": "^19.1.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@typescript-eslint/utils": "^8.32.0",
    "@vitest/eslint-plugin": "^1.1.44",
    "angular-eslint": "^19.3.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-import-x": "^4.11.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsdoc": "^50.6.11",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-yml": "^1.18.0",
    "globals": "^16.1.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "toml-eslint-parser": "^0.10.0",
    "typescript-eslint": "^8.32.0",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@angular/core": "^19.2.9",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/cz-commitlint": "^19.8.0",
    "@fabdeh/eslint-config": "workspace:*",
    "@ngrx/effects": "^19.1.0",
    "@ngrx/operators": "^19.1.0",
    "@ngrx/signals": "^19.1.0",
    "@ngrx/store": "^19.1.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@testing-library/angular": "^17.3.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.14",
    "bumpp": "^10.1.0",
    "changelogithub": "^13.13.0",
    "commitizen": "^4.3.1",
    "eslint": "^9.26.0",
    "eslint-plugin-erasable-syntax-only": "^0.3.1",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "inquirer": "^9.3.7",
    "is-ci": "^4.1.0",
    "jiti": "^2.4.2",
    "nano-staged": "^0.8.0",
    "prettier-plugin-slidev": "^1.0.5",
    "simple-git-hooks": "^2.13.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.3"
  },
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged",
    "commit-msg": "pnpm commitlint --edit ${1}"
  },
  "nano-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
